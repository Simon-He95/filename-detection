{
  "publisher": "simonhe",
  "name": "filename-detection",
  "displayName": "File Name Detection",
  "version": "0.0.4",
  "packageManager": "pnpm@8.10.5",
  "description": "filename-detection",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/filename-detection#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/filename-detection"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/filename-detection/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.84.2"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "filename-detection",
      "properties": {
        "filename-detection.cSpell": {
          "type": "boolean",
          "default": true,
          "description": "Whether to turn on the detection file name"
        }
      }
    }
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@simon_he/eslint-config": "^0.0.6",
    "@types/node": "^18.18.12",
    "@types/vscode": "^1.84.2",
    "@vscode-use/utils": "^0.0.76",
    "bumpp": "^9.2.0",
    "eslint": "^8.54.0",
    "esno": "^0.16.3",
    "pnpm": "^8.10.5",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "typescript": "^5.3.2",
    "typo-js": "^1.2.3",
    "vite": "^4.5.0",
    "vitest": "^0.29.8",
    "vsce": "^2.15.0"
  }
}
