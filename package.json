{
  "publisher": "simonhe",
  "name": "filename-detection",
  "displayName": "File Name Detection",
  "version": "0.0.21",
  "packageManager": "pnpm@8.15.9",
  "description": "filename-detection",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/filename-detection#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/filename-detection"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/filename-detection/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.cjs",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.77.0",
    "node": ">=18.12.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "filename-detection",
      "properties": {
        "filename-detection.cSpell": {
          "type": "boolean",
          "default": true,
          "description": "Whether to turn on the detection file name"
        }
      }
    }
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsdown src/index.ts --external vscode --format cjs",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.1",
    "@simon_he/translate": "^0.0.13",
    "@types/node": "^18.19.79",
    "@types/vscode": "1.77.0",
    "@vscode-use/utils": "^0.1.61",
    "@vscode/vsce": "^3.7.1",
    "bumpp": "^9.11.1",
    "eslint": "^8.57.1",
    "fast-glob": "^3.3.3",
    "lazy-js-utils": "^0.1.49",
    "tsdown": "^0.18.1",
    "typescript": "^5.8.2",
    "typo-js": "^1.3.1",
    "vitest": "^4.0.16"
  }
}
